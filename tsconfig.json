{
  "compilerOptions": {
    "strict": false,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "powerful-hooks": ["src"],
      "powerful-hooks/*": ["src/*", "*"]
    },
    "target": "ES2015",
    "moduleResolution":"Node"
  },
  "include": [".dumirc.ts", "src/**/*"]
}
